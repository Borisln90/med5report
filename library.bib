@inproceedings{Gallagher2003,
abstract = {Pac-Man is a well-known, real-time computer game that provides an interesting platform for research. We describe an initial approach to developing an artificial agent that replaces the human to play a simplified version of Pac-Man. The agent is specified as a simple finite state machine and ruleset. with parameters that control the probability of movement by the agent given the constraints of the maze at some instant of time. In contrast to previous approaches, the agent represents a dynamic strategy for playing Pac-Man, rather than a pre-programmed maze-solving method. The agent adaptively "learns" through the application of population-based incremental learning (PBIL) to adjust the agents' parameters. Experimental results are presented that give insight into some of the complexities of the game, as well as highlighting the limitations and difficulties of the representation of the agent.},
author = {Gallagher, M. and Ryan, A.},
booktitle = {2003 Congress on Evolutionary Computation, CEC 2003 - Proceedings},
doi = {10.1109/CEC.2003.1299397},
file = {:C$\backslash$:/Users/Stefan/Dropbox/5th Semester/Project/Research\_Papers\_Mendeley/gallagher\_pacman\_cig07.pdf:pdf},
isbn = {0-7803-7804-0},
pages = {2462--2469},
title = {{Learning to play Pac-Man: An evolutionary, rule-based approach}},
url = {http://staff.itee.uq.edu.au/marcusg/papers/gallagher\_pacman\_cig07.pdf},
volume = {4},
year = {2003}
}
@inproceedings{Brandstetter2012,
abstract = {During the last years the well-known Ms. Pac Man video$\backslash$ngame has been - and still is - an interesting test bed$\backslash$nfor the research on various concepts from the broad$\backslash$narea of Artificial Intelligence (AI). Among these$\backslash$nconcepts is the use of Genetic Programming (GP) to$\backslash$ncontrol the game from a human player's perspective.$\backslash$nSeveral GP-based approaches have been examined so far,$\backslash$nwhere traditionally they define two types of GP$\backslash$nterminals: one type for information retrieval, the$\backslash$nsecond type for issuing actions (commands) to the game$\backslash$nworld. However, by using these action terminals the$\backslash$ncontroller has to manage actions issued to the game$\backslash$nduring their runtime and to monitor their outcome. In$\backslash$norder to avoid the need for active task management this$\backslash$npaper presents a novel approach for the design of a$\backslash$nGP-based Ms. Pac Man controller: the proposed approach$\backslash$nsolely relies on information retrieval terminals in$\backslash$norder to rate all possible directions of movement at$\backslash$nevery time step during a running game. Based on these$\backslash$nrating values the controller can move the agent through$\backslash$nthe mazes of the the game world of Ms. Pac Man. With$\backslash$nthis design, which forms the main contribution of our$\backslash$nwork, we decrease the overall GP solution complexity by$\backslash$nremoving all action control management tasks from the$\backslash$nsystem. It is demonstrated that by following the$\backslash$nproposed approach such a system can successfully$\backslash$ncontrol an autonomous agent in a computer game$\backslash$nenvironment on the level of an amateur human player.},
author = {Brandstetter, Matthias F and Ahmadi, Samad},
booktitle = {Computational Intelligence and Games (CIG), 2012 IEEE Conference on},
doi = {doi:10.1109/CIG.2012.6374163},
file = {:C$\backslash$:/Users/Stefan/Dropbox/5th Semester/Project/Research\_Papers\_Mendeley/brandstetter.pdf:pdf},
keywords = {AI,Abstracts,Computational intelligence,GP solution complexity,GP-based approaches,Games,Humans,Ms. Pac Man video game,Sociology,Statistics,action control management task removal,action terminals,artificial intelligence,autonomous agent control,computer games,game mazes,genetic algorithms,genetic programming,human player perspective,information retrieval,rating values,reactive control,task management},
pages = {250--256},
title = {{Reactive control of \{Ms. Pac Man\} using Information Retrieval based on Genetic Programming}},
url = {http://ieeexplore.ieee.org.zorac.aub.aau.dk/stamp/stamp.jsp?tp=\&arnumber=6374163},
year = {2012}
}
@article{Lucas2006,
abstract = {Games provide competitive, dynamic environments that make ideal test beds for computational intelligence theories, architectures, and algorithms. Natural evolution can be considered to be a game in which the rewards for an organism that plays a good game of life are the propagation of its genetic material to its successors and its continued survival. In natural evolution, the fitness of an individual is defined with respect to its competitors and collaborators, as well as to the environment. Within the evolutionary computation (EC) literature, this is known as co-evolution and within this paradigm, expert game-playing strategies have been evolved without the need for human expertise.},
